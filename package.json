{
    "name": "@miracledevs/paradigm-ui-web-angularjs",
    "version": "1.4.1",
    "description": "A typescript library that wraps angularjs in a object oriented way, allowing for a cleaner approach to angularjs.",
    "author": "Miracle Devs",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/MiracleDevs/Paradigm.Web.AngularJS.git"
    },
    "scripts": {
        "test": "karma start karma.conf.js",
        "build": "npm install && tsc --project . --incremental",
        "watch": "npm run build -- --watch",
        "build-publish": "npm run build && npm run copy-readme && npm run copy-package",
        "copy-readme": "cp ./readme.md ./dist/",
        "copy-package": "cp ./package.json ./dist/",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\""
    },
    "private": false,
    "keywords": [
        "miracledevs",
        "paradigm",
        "ui",
        "javascript",
        "typescript",
        "angularjs",
        "controller",
        "component",
        "direcive",
        "filter",
        "interceptor",
        "service",
        "spa",
        "single page application",
        "uirouter",
        "translate"
    ],
    "devDependencies": {
        "@miracledevs/paradigm-ui-web-shared": "1.0.5",
        "@types/angular": "1.8.4",
        "@types/angular-animate": "1.5.11",
        "@types/angular-mocks": "1.7.1",
        "@types/angular-sanitize": "1.8.1",
        "@types/angular-translate": "2.16.3",
        "@types/jasmine": "3.10.3",
        "@types/jquery": "3.5.13",
        "@types/node": "^17.0.14",
        "angular-mocks": "1.8.2",
        "clean": "4.0.2",
        "jasmine": "4.0.2",
        "jasmine-core": "4.0.0",
        "karma": "6.3.14",
        "karma-chrome-launcher": "^3.1.0",
        "karma-jasmine": "^4.0.1",
        "karma-nyan-reporter": "^0.2.5",
        "karma-typescript": "^5.5.3",
        "karma-typescript-preprocessor": "^0.4.0",
        "phantomjs-prebuilt": "^2.1.16",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@miracledevs/paradigm-ui-web-shared": "^1.0.5",
        "@uirouter/angularjs": "1.0.30",
        "angular": "1.8.2",
        "angular-animate": "1.8.2",
        "angular-sanitize": "1.8.2",
        "angular-translate": "2.19.0",
        "eonasdan-bootstrap-datetimepicker": "4.17.49",
        "eslint": "^8.8.0",
        "jquery": "3.6.0",
        "mousetrap": "1.6.5",
        "ng-pattern-restrict": "0.2.3",
        "prettier": "^2.5.1",
        "tooltipster": "4.2.8",
        "tslib": "^2.3.1"
    }
}
